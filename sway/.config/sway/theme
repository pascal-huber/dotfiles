
set $fg #efefef
set $bg #333333
set $red #FF0000
set $yellow #dddd00
set $gray #aaaaaa

# class                 border|backgr|text|indicator|child_border
client.focused          $fg $fg $bg $red $fg
client.focused_inactive $bg $bg $yellow $bg $bg
client.unfocused        $bg $bg $gray $bg $bg
client.urgent           $bg $bg $red $bg $bg
